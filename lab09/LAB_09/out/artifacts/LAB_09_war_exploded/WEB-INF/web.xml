<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <filter>
        <filter-name>F1</filter-name>
        <filter-class>F1</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>F1</filter-name>
        <url-pattern>/Ccc</url-pattern>
    </filter-mapping>

        <filter>
        <filter-name>F2</filter-name>
        <filter-class>F2</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>F2</filter-name>
        <url-pattern>/Ccc</url-pattern>
    </filter-mapping>

    <filter>
        <filter-name>F3</filter-name>
        <filter-class>F3</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>F3</filter-name>
        <url-pattern>/Ccc</url-pattern>
    </filter-mapping>

    <filter>
        <filter-name>Jsp</filter-name>
        <filter-class>Jsp</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>Jsp</filter-name>
        <url-pattern>/Ccc.jsp</url-pattern>
    </filter-mapping>


    <listener>
        <listener-class>L1</listener-class>
    </listener>

    <listener>
        <listener-class>L2</listener-class>
    </listener>

</web-app>
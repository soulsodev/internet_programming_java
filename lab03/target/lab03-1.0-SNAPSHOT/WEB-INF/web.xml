<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <!--Redirect-->
    <servlet>
        <servlet-name>Sss</servlet-name>
        <servlet-class>Sss</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>Ggg</servlet-name>
        <servlet-class>Ggg</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Sss</servlet-name>
        <url-pattern>/Sss</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Ggg</servlet-name>
        <url-pattern>/Ggg</url-pattern>
    </servlet-mapping>

    <!--Forward-->
    <servlet>
        <servlet-name>SssForward</servlet-name>
        <servlet-class>SssForward</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>GggForward</servlet-name>
        <servlet-class>GggForward</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SssForward</servlet-name>
        <url-pattern>/SssForward</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GggForward</servlet-name>
        <url-pattern>/GggForward</url-pattern>
    </servlet-mapping>

    <!--HttpClient-->
    <servlet>
        <servlet-name>SssHttpClient</servlet-name>
        <servlet-class>SssHttpClient</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>GggHttpClient</servlet-name>
        <servlet-class>GggHttpClient</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SssHttpClient</servlet-name>
        <url-pattern>/SssHttpClient</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GggHttpClient</servlet-name>
        <url-pattern>/GggHttpClient</url-pattern>
    </servlet-mapping>


</web-app>